#include<iostream>
#include<windows.h>
#include<conio.h>

using namespace std;

//funcion para mover caracteres por medio dle teclado
void gotoxy(int x, int y){
HANDLE hcon = GetStdHandle(STD_OUTPUT_HANDLE);
 COORD dwPos;
 dwPos.X = x;
 dwPos.Y = y;
 SetConsoleCursorPosition(hcon, dwPos);
}



class enemigo{
	private:
		public:
			void pintar();
};

void enemigo::pintar()
{
	gotoxy(5,5);
	cout<<"O";
}

int main ()
{
	enemigo a;
	
	a.pintar();
}

